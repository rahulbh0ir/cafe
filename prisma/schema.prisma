generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Reservation {
  id String @id @default(uuid())
  name String
  phone String
  email String
  date String
  time String
  createdAt DateTime @default(now())
  
}